<?xml version='1.0' encoding='utf-8'?>
<widget id="es.uc3m.inemuri" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>InemuriApp</name>
    <description>Una aplicación para dormir en el transporte público y no saltarse la parada.
    </description>
    <author email="100373464@alumnos.uc3m.es" href="http://uc3m.es">
        Enrique de Aramburu
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
	
	<icon src="www/img/logo.png" />
</widget>
